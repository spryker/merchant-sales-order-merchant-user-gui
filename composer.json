{
  "name": "spryker/merchant-sales-order-merchant-user-gui",
  "type": "library",
  "description": "MerchantSalesOrderMerchantUserGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/customer": "^7.0.0",
    "spryker/gui": "^3.33.0",
    "spryker/kernel": "^3.33.0",
    "spryker/merchant-oms": "^0.3.4",
    "spryker/merchant-sales-order": "^0.7.3",
    "spryker/merchant-user": "^0.5.3",
    "spryker/money": "^2.0.0",
    "spryker/sales": "^11.0.0",
    "spryker/shipment": "^8.0.0",
    "spryker/state-machine": "^2.12.0",
    "spryker/symfony": "^3.0.0",
    "spryker/util-date-time": "^1.0.0",
    "spryker/util-sanitize": "^2.0.0",
    "spryker/util-text": "^1.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/merchant": "*",
    "spryker/oms": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
